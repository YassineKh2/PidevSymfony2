{% extends 'base.html.twig' %}

{% block title %}ReactionPublication index{% endblock %}

{% block body %}
    <h1>ReactionPublication index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for reaction_publication in reaction_publications %}
            <tr>
                <td>{{ reaction_publication.id }}</td>
                <td>
                    <a href="{{ path('app_reaction_publication_show', {'id': reaction_publication.id}) }}">show</a>
                    <a href="{{ path('app_reaction_publication_edit', {'id': reaction_publication.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_reaction_publication_new') }}">Create new</a>
{% endblock %}
